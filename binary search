
#include <stdio.h>

int main()
{
    int a[100];
    int N;
    int target;
    int i;
    int found=0;
    printf("enter the size of the array");
    scanf("%d",&N);
    for(i=0;i<N;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("enter the target");
    scanf("%d",&target);
    for(i=0;i<N;i++)
    {
        if(a[i]==target)
        {
            found=1;
            break;
        }
    }
    if(found!=0)
    {
        printf("the number is found  at %d ",i);
    }
    else
    {
        printf("the number is not found");
    }
}
